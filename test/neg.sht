// -*- C++ -*-

ShEnvironment::optimizationLevel = 2;

ShProgram shader = SH_BEGIN_VERTEX_PROGRAM {
  ShInputPosition3f input;

  ShOutputPosition3f output;

  ShPosition3f temp1, temp2, temp3;

  temp1 = input;
  temp2 = temp1 * -temp1;
  temp3 = -temp2 * temp1;

  output = temp3;
} SH_END_PROGRAM;
