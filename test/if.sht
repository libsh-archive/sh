struct F {
static ShAttrib1f f()
{
  ShAttrib1f ret; NAME(ret);
  ShAttrib2f x(2.0, 3.0); NAME(x);

  ret(0) = x(1);
  
  SH_IF(x(0)) {
    ret = x(0) + x(1);
  } SH_ELSE {
    ret = x(1) * x(1);
  } SH_ENDIF;
  return ret;
}
};

ShProgram shader = SH_BEGIN_VERTEX_PROGRAM {
  ShInputAttrib1f in; NAME(in);
  ShAttrib1f temp; NAME(temp);
  ShOutputAttrib1f out; NAME(out);
  
  temp = in * in;
  SH_IF(temp) {
    out = temp + temp;
    SH_IF(in + in) {
      out = temp + in;
    } SH_ENDIF;
  } SH_ELSE {
    out = ShAttrib1f(0.0);
  } SH_ENDIF;
  temp = in + in;
} SH_END_PROGRAM;
