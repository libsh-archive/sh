// -*- C++ -*-
ShProgram shader = SH_BEGIN_VERTEX_PROGRAM {
  
  ShInputAttrib4f in1; NAME(in1);
  ShOutputAttrib4f out1; NAME(out1);
  ShOutputAttrib1f out2; NAME(out2);

  out1 = in1 * 1.0;
  out1 = 2.0 * in1;
  out2 = in1(1) < 3.0;
  out2 += 4.0;
  out1 *= 5.0;
  out2 = 6.0;
  
  
} SH_END_PROGRAM;


